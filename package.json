{
  "name": "hdty-framework-w4",
  "version": "2.0.1",
  "description": "华动泰越|基于vue2、webpack4、element-ui的快速开发框架，node环境升级到14，更新部分依赖版本",
  "author": "zhaozhiyong",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vue-cli-service serve --no-lint",
    "serve": "nvm use 14 && cross-env NODE_OPTIONS=--max-old-space-size=4096 vue-cli-service serve --no-lint",
    "start": "npm run dev",
    "startNVM": "npm run useNVM && npm run dev",
    "build": "node scripts/version-updater.js && vue-cli-service build --no-lint",
    "buildNVM": "npm run useNVM && npm run build",
    "build:no-version": "vue-cli-service build --no-lint",
    "lint": "vue-cli-service lint",
    "currentNVM": "nvm current",
    "listNVM": "nvm list",
    "useNVM": "nvm use 14.21.3"
  },
